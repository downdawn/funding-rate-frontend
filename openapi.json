          {
            "name": "exchange",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "description": "交易所名称",
              "title": "Exchange"
            },
            "description": "交易所名称"
          },
          {
            "name": "symbol",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "description": "交易对符号",
              "title": "Symbol"
            },
            "description": "交易对符号"
          },
          {
            "name": "start_time",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "description": "开始时间戳",
              "title": "Start Time"
            },
            "description": "开始时间戳"
          },
          {
            "name": "end_time",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "description": "结束时间戳",
              "title": "End Time"
            },
            "description": "结束时间戳"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "maximum": 1000,
              "minimum": 1,
              "description": "返回记录数量限制",
              "default": 100,
              "title": "Limit"
            },
            "description": "返回记录数量限制"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FundingRateListResponse"
